<div class="container">
<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>

<div><label for="inputName">名前</label>
<%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%></div>

<div><label for="inputImage">プロフィール画像</label><br>
<%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %></div>

<div><%= f.submit "変更を保存" %></div>
<% end %>
</div>